{
  "name": "tauri-app",
  "private": true,
  "version": "1.0.0-rc-1",
  "type": "module",
  "scripts": {
    "build": "yarn tauri build",
    "build-intel": "yarn tauri build --target x86_64-apple-darwin",
    "dev": "yarn tauri dev",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri-build": "run-s tauri-build:*",
    "tauri-build:css": "yarn tailwindcss -i ./src/main.css -o ./dist/main.css --minify",
    "tauri-build:vite": "vite build",
    "tauri-dev": "run-p tauri-dev:*",
    "tauri-dev:css": "yarn tailwindcss -i ./src/main.css -o ./dist/main.css --watch",
    "tauri-dev:vite": "vite"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.2.0",
    "axios": "^1.3.4",
    "axios-tauri-adapter": "^0.2.0",
    "vue": "^3.2.45",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.2.3",
    "@vitejs/plugin-vue": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.4.0",
    "tailwindcss": "^3.2.7",
    "vite": "^4.0.0"
  }
}
